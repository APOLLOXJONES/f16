  	<layout>vbox</layout>
  	<draggable>true</draggable>
    <resizable>false</resizable>
    <width>700</width>
    <!--<height>250</height>-->
    
    <nasal>
        <open><![CDATA[
        	setprop("f16/preplanning-status", "");
			var s = steerpoints.getNumber(555);
			if (s != nil) {
				setprop("f16/avionics/pilot-aid/bulls-eye-lat", s.lat);
				setprop("f16/avionics/pilot-aid/bulls-eye-lon", s.lon);
			}
			s = steerpoints.getNumber(300);# 
			if (s != nil) {
				setprop("f16/avionics/pilot-aid/c1-lat", s.lat);
				setprop("f16/avionics/pilot-aid/c1-lon", s.lon);
				setprop("f16/avionics/pilot-aid/c1-rad", s.radius);
				setprop("f16/avionics/pilot-aid/c1-typ", s.type);
				setprop("f16/avionics/pilot-aid/c1-col", s.color==0?"RED":(s.color==1?"YEL":"GRN"));
			}
			s = steerpoints.getNumber(301);# 
			if (s != nil) {
				setprop("f16/avionics/pilot-aid/c2-lat", s.lat);
				setprop("f16/avionics/pilot-aid/c2-lon", s.lon);
				setprop("f16/avionics/pilot-aid/c2-rad", s.radius);
				setprop("f16/avionics/pilot-aid/c2-typ", s.type);
				setprop("f16/avionics/pilot-aid/c2-col", s.color==0?"RED":(s.color==1?"YEL":"GRN"));
			}
			s = steerpoints.getNumber(302);# 
			if (s != nil) {
				setprop("f16/avionics/pilot-aid/c3-lat", s.lat);
				setprop("f16/avionics/pilot-aid/c3-lon", s.lon);
				setprop("f16/avionics/pilot-aid/c3-rad", s.radius);
				setprop("f16/avionics/pilot-aid/c3-typ", s.type);
				setprop("f16/avionics/pilot-aid/c3-col", s.color==0?"RED":(s.color==1?"YEL":"GRN"));
			}
			s = steerpoints.getNumber(303);# 
			if (s != nil) {
				setprop("f16/avionics/pilot-aid/c4-lat", s.lat);
				setprop("f16/avionics/pilot-aid/c4-lon", s.lon);
				setprop("f16/avionics/pilot-aid/c4-rad", s.radius);
				setprop("f16/avionics/pilot-aid/c4-typ", s.type);
				setprop("f16/avionics/pilot-aid/c4-col", s.color==0?"RED":(s.color==1?"YEL":"GRN"));
			}
			s = steerpoints.getNumber(304);# 
			if (s != nil) {
				setprop("f16/avionics/pilot-aid/c5-lat", s.lat);
				setprop("f16/avionics/pilot-aid/c5-lon", s.lon);
				setprop("f16/avionics/pilot-aid/c5-rad", s.radius);
				setprop("f16/avionics/pilot-aid/c5-typ", s.type);
				setprop("f16/avionics/pilot-aid/c5-col", s.color==0?"RED":(s.color==1?"YEL":"GRN"));
			}
			s = steerpoints.getNumber(305);# 
			if (s != nil) {
				setprop("f16/avionics/pilot-aid/c6-lat", s.lat);
				setprop("f16/avionics/pilot-aid/c6-lon", s.lon);
				setprop("f16/avionics/pilot-aid/c6-rad", s.radius);
				setprop("f16/avionics/pilot-aid/c6-typ", s.type);
				setprop("f16/avionics/pilot-aid/c6-col", s.color==0?"RED":(s.color==1?"YEL":"GRN"));
			}
			
            var load_line1 = func(path) {
                steerpoints.loadLine(0,path.getValue());
            }
            
            var load_line2 = func(path) {
                steerpoints.loadLine(1,path.getValue());
            }

            #var dlg = props.globals.getNode("/sim/gui/dialogs/route-manager", 1);
            
            var load_stpts = func(path) {
                steerpoints.loadSTPTs(path.getValue());
            }
            var save_stpts = func(path) {
                steerpoints.saveSTPTs(path.getValue());
            }

            var defaultDirInFileSelector = getprop("/sim/fg-home") ~ "/Export";

            var file_selector_1 = gui.FileSelector.new(
              callback: load_line1, title: "Load Lines 1", button: "Load",
              dir: defaultDirInFileSelector, dotfiles: 1, pattern: ["*.gpx","*.fgfp"]);
            var file_selector_2 = gui.FileSelector.new(
              callback: load_line2, title: "Load Lines 2", button: "Load",
              dir: defaultDirInFileSelector, dotfiles: 1, pattern: ["*.gpx","*.fgfp"]);

             var file_selector_dtc = gui.FileSelector.new(
              callback: load_stpts, title: "Load data", button: "Load",
              dir: defaultDirInFileSelector, dotfiles: 1, pattern: ["*.f16dtc"]);
            var save_selector_dtc = gui.FileSelector.new(
              callback: save_stpts, title: "Save data", button: "Save",
              dir: defaultDirInFileSelector, dotfiles: 1, file: "f16-stpt-iff-and-dl.f16dtc", pattern: ["*.f16dtc"]);
            
        ]]></open>

        <close><![CDATA[
            file_selector_1.del();            
            file_selector_2.del();            
            file_selector_dtc.del();            
            save_selector_dtc.del();
        ]]></close>
