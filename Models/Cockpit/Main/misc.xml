<?xml version="1.0"?>
<PropertyList>
    <path>misc.ac</path>
    <effect>
        <name>models-effects-f16panel-left</name>
        <inherits-from>Models/Effects/f16panel-left</inherits-from>
        <object-name>MISC_1-block-50</object-name>
        <object-name>MISC_1-block-15</object-name>
    </effect>
    <animation>
        <type>material</type>
        <object-name>block-15</object-name>
        <object-name>block-50</object-name>
        <emission>
            <factor-prop>controls/lighting/lighting-panel/flood-inst-pnl</factor-prop>
            <red>0.45</red>
            <green>0.34</green>
            <blue>0.18</blue>
        </emission>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>alt-rel-button</object-name>
        <property>controls/flight/alt-rel-button</property>
        <factor>1</factor>
        <center>
            <x-m>1</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0.15</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <release>
            <binding>
                <command>property-adjust</command>
                <property>controls/flight/alt-rel-button</property>
                <min>0</min>
                <max>0</max>
            </binding>
        </release>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/flight/alt-rel-button</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>autopilot-switch</object-name>
        <property>f16/fcs/autopilot-on</property>
        <factor>45</factor>
        <axis>
            <object-name>autopilot-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>f16/fcs/autopilot-on</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>laser-arm-switch</object-name>
        <property>controls/armament/laser-arm-dmd</property>
        <factor>60</factor>
        <axis>
            <object-name>laser-arm-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/armament/laser-arm-dmd</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>master-arm-switch</object-name>
        <condition>
            <or>
                <greater-than>
                    <property>controls/armament/master-arm-cover-open</property>
                    <value>0</value>
                </greater-than>
                <greater-than>
                    <property>sim/variant-id</property>
                    <value>5</value>
                </greater-than>
            </or>
        </condition>
        <property>controls/armament/master-arm</property>
        <factor>45</factor>
        <axis>
            <object-name>master-arm-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/armament/master-arm</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>roll-hold-switch</object-name>
        <condition>
            <greater-than-equals>
                <property>sim/variant-id</property>
                <value>2</value>
            </greater-than-equals>
        </condition>
        <property>f16/fcs/switch-roll</property>
        <factor>45</factor>
        <axis>
            <object-name>roll-hold-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>f16/fcs/switch-roll</property>
                <factor>1</factor>
                <min>-1</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>pitch-hold-switch</object-name>
        <condition>
            <greater-than-equals>
                <property>sim/variant-id</property>
                <value>2</value>
            </greater-than-equals>
        </condition>
        <property>f16/fcs/switch-pitch</property>
        <factor>45</factor>
        <axis>
            <object-name>pitch-hold-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>f16/fcs/switch-pitch</property>
                <factor>1</factor>
                <min>-1</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>master-arm-cover</object-name>
        <condition>
            <equals>
                <property>controls/armament/master-arm</property>
                <value>0</value>
            </equals>
        </condition>
        <property>controls/armament/master-arm-cover-open</property>
        <factor>-90</factor>
        <axis>
            <object-name>master-arm-cover-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/armament/master-arm-cover-open</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>quiet-switch</object-name>
        <property>controls/armament/quiet</property>
        <offset-deg>-30</offset-deg>
        <factor>60</factor>
        <axis>
            <object-name>quiet-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/armament/quiet</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>pitch-switch-b15</object-name>
        <condition>
            <less-than>
                <property>sim/variant-id</property>
                <value>2</value>
            </less-than>
        </condition>
        <property>f16/fcs/switch-pitch-hold</property>
        <offset-deg>-30</offset-deg>
        <factor>60</factor>
        <axis>
            <object-name>pitch-switch-b15-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>f16/fcs/switch-pitch-hold</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>roll-switch-b15</object-name>
        <condition>
            <less-than>
                <property>sim/variant-id</property>
                <value>2</value>
            </less-than>
        </condition>
        <property>f16/fcs/switch-roll-hold</property>
        <offset-deg>-30</offset-deg>
        <factor>60</factor>
        <axis>
            <object-name>roll-switch-b15-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>f16/fcs/switch-roll-hold</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>stby-off</object-name>
        <object-name>stby-on</object-name>
        <object-name>active-on</object-name>
        <object-name>active-off</object-name>
        <object-name>adv-mode-button</object-name>
        <property>f16/fcs/adv-mode-sel</property>
        <factor>1</factor>
        <center>
            <x-m>1</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0.15</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>f16/fcs/adv-mode-sel</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>select</type>
        <object-name>active-off</object-name>
        <condition>
            <equals>
                <property>f16/fcs/adv-mode</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>active-on</object-name>
        <condition>
            <greater-than>
                <property>f16/fcs/adv-mode</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>autopilot-switch</object-name>
        <condition>
            <less-than>
                <property>sim/variant-id</property>
                <value>2</value>
            </less-than>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>block-15</object-name>
        <condition>
            <less-than>
                <property>sim/variant-id</property>
                <value>2</value>
            </less-than>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>block-50</object-name>
        <condition>
            <greater-than-equals>
                <property>sim/variant-id</property>
                <value>2</value>
            </greater-than-equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>ecm-enbl-off</object-name>
        <condition>
            <equals>
                <property>controls/armament/ecm-enabled</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>ecm-enbl-on</object-name>
        <condition>
            <greater-than>
                <property>controls/armament/ecm-enabled</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>master-arm-cover</object-name>
        <condition>
            <less-than-equals>
                <property>sim/variant-id</property>
                <value>5</value>
            </less-than-equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>pitch-hold-switch</object-name>
        <condition>
            <greater-than-equals>
                <property>sim/variant-id</property>
                <value>2</value>
            </greater-than-equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>pitch-switch-b15</object-name>
        <condition>
            <less-than>
                <property>sim/variant-id</property>
                <value>2</value>
            </less-than>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>roll-hold-switch</object-name>
        <condition>
            <greater-than-equals>
                <property>sim/variant-id</property>
                <value>2</value>
            </greater-than-equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>roll-switch-b15</object-name>
        <condition>
            <less-than>
                <property>sim/variant-id</property>
                <value>2</value>
            </less-than>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>stby-off</object-name>
        <condition>
            <equals>
                <property>f16/fcs/stby-mode</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>stby-on</object-name>
        <condition>
            <greater-than>
                <property>f16/fcs/stby-mode</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>
</PropertyList>

