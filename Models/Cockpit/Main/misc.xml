<?xml version="1.0"?>
<PropertyList>
    <path>misc.ac</path>
    <animation>
        <type>knob</type>
        <object-name>drag-chute-cover</object-name>
        <property>controls/flight/drag-chute-cover-dmd</property>
        <factor>-90</factor>
        <axis>
            <object-name>drag-chute-cover-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/flight/drag-chute-cover-dmd</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>drag-chute-switch</object-name>
        <condition>
            <greater-than>
                <property>controls/flight/drag-chute-cover-dmd</property>
                <value>0</value>
            </greater-than>
        </condition>
        <property>controls/flight/drag-chute-dmd</property>
        <factor>-45</factor>
        <axis>
            <object-name>drag-chute-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/flight/drag-chute-dmd</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>laser-arm-switch</object-name>
        <property>controls/armament/laser-arm-dmd</property>
        <factor>45</factor>
        <axis>
            <object-name>laser-arm-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/armament/laser-arm-dmd</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>alt-rel-button</object-name>
        <property>controls/flight/alt-rel-button</property>
        <factor>1</factor>
        <center>
            <x-m>1</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0.15</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <release>
            <binding>
                <command>property-adjust</command>
                <property>controls/flight/alt-rel-button</property>
                <min>0</min>
                <max>0</max>
            </binding>
        </release>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/flight/alt-rel-button</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>master-arm-switch</object-name>
        <property>controls/armament/master-arm</property>
        <factor>45</factor>
        <axis>
            <object-name>master-arm-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/armament/master-arm</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>hud-displ-switch</object-name>
        <property>controls/HUD/display-on</property>
        <factor>60</factor>
        <axis>
            <object-name>hud-displ-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/HUD/display-on</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>roll-hold-switch</object-name>
        <property>f16/fcs/switch-roll</property>
        <factor>45</factor>
        <axis>
            <object-name>roll-hold-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>f16/fcs/switch-roll</property>
                <factor>1</factor>
                <min>-1</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>pitch-hold-switch</object-name>
        <property>f16/fcs/switch-pitch</property>
        <factor>45</factor>
        <axis>
            <object-name>pitch-hold-switch-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>f16/fcs/switch-pitch</property>
                <factor>1</factor>
                <min>-1</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>material</type>
        <object-name>panel</object-name>
        <emission>
            <factor-prop>controls/lighting/lighting-panel/flood-inst-pnl</factor-prop>
            <red>0.45</red>
            <green>0.34</green>
            <blue>0.18</blue>
        </emission>
    </animation>
</PropertyList>

