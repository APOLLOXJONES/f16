<?xml version="1.0"?>

<PropertyList>

 <path>pilot_externalview.ac</path>

 <animation>
        <type>material</type>
        <object-name>helmet</object-name>
        <object-name>patchL</object-name>
        <object-name>patchR</object-name>
        <object-name>patchC</object-name>
        <property-base>sim/model/livery-patch</property-base>
        <texture-prop>texture</texture-prop>
        <texture>Liveries/patch.default.png</texture>
    </animation>
    
    <effect>
      <inherits-from>Effects/model-combined-transparent</inherits-from>
      <parameters>
			<rain-enabled type="int">0</rain-enabled>
			<snow-enabled type="int">0</snow-enabled>
			<ambient-correction type="float">0.00</ambient-correction>
			<lightmap-enabled type="int">0</lightmap-enabled>
			<reflection-enabled type="int">1</reflection-enabled>			
			<reflection-correction type="float">0.25</reflection-correction>
			<reflection-fresnel type="float">0.0</reflection-fresnel>
			<reflection-fresnel-factor type="float">0.0</reflection-fresnel-factor>
			<reflection-rainbow type="float">0.25</reflection-rainbow>
			<reflection-noise type="float">0.0</reflection-noise>
			<reflection-dynamic type="int">1</reflection-dynamic>
			<reflection-type type="int">1</reflection-type>
			<texture n= "0" ><type>black</type></texture>
			<texture n= "5" >
				<type>cubemap</type>
				<images>
				  <positive-x>Aircraft/Generic/Effects/fgfs-sky2/1.png</positive-x>
				  <negative-x>Aircraft/Generic/Effects/fgfs-sky2/4.png</negative-x>
				  <positive-y>Aircraft/Generic/Effects/fgfs-sky2/2.png</positive-y>
				  <negative-y>Aircraft/Generic/Effects/fgfs-sky2/3.png</negative-y>
				  <positive-z>Aircraft/Generic/Effects/fgfs-sky2/6.png</positive-z>
				  <negative-z>Aircraft/Generic/Effects/fgfs-sky2/5.png</negative-z>
				</images>
			</texture>
			<rendering-hint>transparent</rendering-hint>
			<transparent>true</transparent>
			<render-bin>
			  <bin-number>10</bin-number>
			  <bin-name>DepthSortedBin</bin-name>
			</render-bin>
      </parameters>
      <object-name>L-visor</object-name><!-- inner sidewalls of outlet -->
 </effect>

</PropertyList>
